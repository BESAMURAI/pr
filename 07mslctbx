import javax.swing.*;

public class Listexuh {
    public Listexuh() {
        JFrame f = new JFrame();
        DefaultListModel l1 = new DefaultListModel();
        l1.addElement("BCA");
        l1.addElement("BSC.CS");
        l1.addElement("BSC.IT");
        l1.addElement("BSC,CT");
        l1.addElement("BSC");
        JList list = new JList(l1);
        f.add(new JScrollPane(list));
        f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        f.setSize(400, 400);
        f.setVisible(true);
    }

    public static void main(String[] args) {
        new Listexuh();
    }
}
