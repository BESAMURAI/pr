import javax.swing.*;

public class test7{
	test7(){
		JFrame f=new JFrame();
		DefaultListModel l1=new DefaultListModel();
		l1.addElement("BCA");
		l1.addElement("BSC");
		l1.addElement("BA ENG");
		l1.addElement("PSYCHO");
		
		JList list=new JList(l1);
		list.setBounds(100,100,75,75);
		
		f.add(list);
		f.setSize(400,400);
		f.setLayout(null);
		f.setVisible(true);
	}
	
	public static void main(String args[]){
		new test7();
	}
}
